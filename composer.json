{
    "name": "nidhalkratos/laravel-coconut-v2",
    "description": "A Wrapper around coconut transcoding library for laravel",
    "keywords": [
        "nidhalkratos",
        "laravel-coconut-v2"
    ],
    "homepage": "https://github.com/nidhalkratos/laravel-coconut-v2",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nidhal Abidi",
            "email": "nidhalkratos@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "opencoconut/coconut": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Nidhalkratos\\LaravelCoconut\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nidhalkratos\\LaravelCoconut\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidhalkratos\\LaravelCoconut\\CoconutProvider"
            ]
        }
    }
}
